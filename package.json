{
	"name": "fontoxpath",
	"version": "3.0.0",
	"description": "A minimalistic XPath 3.1 engine in JavaScript",
	"main": "dist/fontoxpath.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"prepare": "npm run build",
		"build-dist": "node ./build.js",
		"build": "tsickle -- --outDir built",
		"test": "nyc ts-mocha --require source-map-support/register --paths test/specs/**/*.tests.* -p test/tsconfig.json",
		"ci-test": "nyc --no-clean ts-mocha --paths test/specs --recursive -p test/tsconfig.json",
		"integrationtests": "ts-mocha --paths --recursive -p test/tsconfig.json test/specs/parsing --dist",
		"qt3tests": "ts-mocha --paths -p test/tsconfig.json test/qt3tests.js",
		"ci-qt3tests": "nyc --no-clean ts-mocha --paths -p test/tsconfig.json test/qt3tests.js",
		"qt3testsxqueryx": "ts-mocha --paths -p test/tsconfig.json test/qt3testsXQueryX.js",
		"xqutstests": "ts-mocha --paths -p test/tsconfig.json test/xqutsTests.js",
		"ci-xqutstests": "nyc --no-clean ts-mocha --paths -p test/tsconfig.json test/xqutsTests.js",
		"xqutstestsxqueryx": "ts-mocha --paths -p test/tsconfig.json test/xqutsTestsXQueryX.js",
		"coverage": "nyc report --reporter=text-lcov | coveralls",
		"alltests": "ts-mocha --paths --recursive -p test/tsconfig.json test/specs/ test/qt3tests test/qt3testsXQueryX test/xqutsTests test/xqutsTestsXQueryX",
		"ts-build": "tslint -p tsconfig.json && tsc",
		"ts-debug": ""
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:FontoXML/fontoxpath.git"
	},
	"files": [
		"dist/fontoxpath.js",
		"dist/fontoxpath.js.map"
	],
	"keywords": [
		"FontoXPath",
		"fontoxpath",
		"FontoXML",
		"XPath",
		"XPath3.1",
		"xpath",
		"xpath3.1",
		"W3C",
		"w3c",
		"XML",
		"xml",
		"XQuery",
		"xquery",
		"XQuery Update Facility",
		"xquery update facility",
		"XQUF",
		"xquf",
		"fontoxml"
	],
	"author": "The FontoXML team",
	"license": "MIT",
	"homepage": "https://github.com/FontoXML/fontoxpath",
	"devDependencies": {
		"@types/chai": "^4.1.7",
		"@types/mocha": "^5.2.5",
		"@types/node": "^10.12.18",
		"@types/sinon": "^7.0.3",
		"chai": "^4.1.2",
		"coveralls": "^3.0.2",
		"google-closure-compiler": "^20181125.0.1",
		"istanbul": "^0.4.5",
		"mocha": "^5.2.0",
		"module-alias": "^2.1.0",
		"nyc": "^13.1.0",
		"pegjs": "^0.10.0",
		"sinon": "^6.3.4",
		"sinon-chai": "^3.2.0",
		"slimdom": "^2.1.3",
		"slimdom-sax-parser": "^1.1.3",
		"source-map-support": "^0.5.9",
		"ts-mocha": "^2.0.0",
		"ts-node": "^7.0.1",
		"tsconfig-paths": "^3.7.0",
		"tsickle": "^0.34.0",
		"tslint": "^5.12.0",
		"typescript": "^3.1.6",
		"uglify-js": "^3.4.9"
	},
	"dependencies": {}
}
